{
  "name": "vl-ui-textarea",
  "version": "3.1.0",
  "description": "Gebruik de vl-ui-textarea component om een tekstveld met meerdere lijnen te hebben.",
  "author": "DIDM",
  "license": "MIT",
  "main": "./src/index.js",
  "exports": {
    "require": "./index.js",
    "import": "./src/index.js",
    "node": "./index.js",
    "default": "./index.js"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-textarea#readme",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-textarea/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-textarea.git"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "npm explore vl-ui-util -- npm run test:component",
    "test:grid": "npm explore vl-ui-util -- npm run test:component:grid",
    "test:unit": "npm explore vl-ui-util -- npm run test:component:unit",
    "test:unit:grid": "npm explore vl-ui-util -- npm run test:component:unit:grid",
    "test:e2e": "npm explore vl-ui-util -- npm run test:component:e2e",
    "test:e2e:grid": "npm explore vl-ui-util -- npm run test:component:e2e:grid",
    "release": "npm explore vl-ui-util -- npm run release",
    "release:prepare": "npm explore vl-ui-util -- npm run release-component:prepare $(pwd) textarea",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "tinymce": "^5.4.1",
    "vl-ui-button": "github:milieuinfo/webcomponent-vl-ui-button#3.11.5",
    "vl-ui-core": "github:milieuinfo/webcomponent-vl-ui-core#3.11.5",
    "vl-ui-form-grid": "github:milieuinfo/webcomponent-vl-ui-form-grid#3.11.5",
    "vl-ui-form-message": "github:milieuinfo/webcomponent-vl-ui-form-message#3.11.5",
    "vl-ui-modal": "github:milieuinfo/webcomponent-vl-ui-modal#3.11.5",
    "vl-ui-input-field": "github:milieuinfo/webcomponent-vl-ui-input-field#3.11.5"
  },
  "peerDependencies": {
    "tinymce": "5.4.1",
    "vl-ui-button": "github:milieuinfo/webcomponent-vl-ui-button#3.11.5",
    "vl-ui-core": "github:milieuinfo/webcomponent-vl-ui-core#3.11.5",
    "vl-ui-form-grid": "github:milieuinfo/webcomponent-vl-ui-form-grid#3.11.5",
    "vl-ui-form-message": "github:milieuinfo/webcomponent-vl-ui-form-message#3.11.5",
    "vl-ui-modal": "github:milieuinfo/webcomponent-vl-ui-modal#3.11.5",
    "vl-ui-input-field": "github:milieuinfo/webcomponent-vl-ui-input-field#3.11.5"
  },
  "devDependencies": {
    "@govflanders/vl-ui-button": "^3.11.5",
    "@govflanders/vl-ui-textarea": "^3.11.5",
    "@govflanders/vl-ui-titles": "^3.11.5",
    "@govflanders/vl-ui-typography": "^3.11.5",
    "vl-ui-icon": "github:milieuinfo/webcomponent-vl-ui-icon#3.11.5",
    "vl-ui-util": "github:milieuinfo/webcomponent-vl-ui-util#3.11.5"
  }
}
