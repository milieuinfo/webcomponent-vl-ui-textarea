{
  "name": "vl-ui-textarea",
  "version": "0.1.0",
  "description": "Gebruik de vl-ui-textarea component om een tekstveld met meerdere lijnen te hebben.",
  "main": "vl-textarea.js",
  "scripts": {
    "prepare": "npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-textarea.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-textarea.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-textarea/$release_version/style.css' vl-textarea.js",
    "release:prepare:commit": "git add -f vl-textarea.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "vl-ui-core": "1.3.3"
  },
  "peerDependencies": {
    "vl-ui-core": "1.3.3"
  },
  "devDependencies": {
    "@govflanders/vl-ui-textarea": "3.7.2",
    "http-server": "indexzero/http-server#use_ecstatic_v4",
    "np": "3.0.4",
    "replace": "1.0.0",
    "uglify-js-es6": "2.8.9",
    "vl-ui-util": "1.0.1",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-textarea.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-textarea/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-textarea#readme"
}
